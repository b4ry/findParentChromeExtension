{
  "name": "Find Parent",
  "description": "Find the element's first relative/absolute parent",
  "version": "1.0",
  "manifest_version": 3,
  "background": {
    "service_worker": "scripts/background.js"
  },
  "permissions": ["storage", "activeTab", "scripting"],
  "action": {
    "default_popup": "html/popup.html"
  }
}
